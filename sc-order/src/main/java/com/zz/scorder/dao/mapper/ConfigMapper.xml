<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zz.scorder.dao.ConfigMapper">
    <select id="selectByCardCode" resultType="com.zz.scorder.entity.ConfigEntity">
        select * from card_code_dict where card_code = #{cardCode}
    </select>
</mapper>